    "@types/express": "^4.17.2",
    "@types/lodash-es": "^4.17.3",
    "@types/wx-js-sdk-dt": "^1.4.2",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "env-status": "^1.3.1",
    "eslint": "^6.8.0",
    "eslint-config-preact": "^1.1.1",
    "gulp": "^3.9.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
