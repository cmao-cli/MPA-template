{
  "name": "your-project-name",
  "version": "0.1.0",
  "private": true,
  "description": "your-project-name",
  "scripts": {
    "commit": "git-cz",
    "clean": "gulp clean:cache && gulp clean:build",
    "build": "gulp build && env-status --gen",
    "build:docker": "NODE_ENV=$front_env env-build-validate && NODE_ENV=$front_env gulp build && env-status --gen",
    "start": "./bin/start.sh",
    "start:docker": "NODE_ENV=$front_env node www/build/app.js",
    "mockup-server": "mockup-server -p 4081",
    "tsc-check": "tsc --project tsconfig.json --outDir build/.tsc-check/ && tsc --project www/tsconfig.json --outDir www/build/.tsc-check/",
    "stylelint": "stylelint src/**/*.scss",
    "stylelint:fix": "stylelint src/**/*.scss --fix",
    "eslint": "eslint --ext ts,tsx src types www/src www/types",
    "eslint:fix": "eslint --ext ts,tsx src types www/src www/types --fix"
  },
  "dependencies": {
    "@cmao/iris": "^0.6.12",
    "@cmao/report": "^2.0.5",
    "closest": "^0.0.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "core-js": "^2.6.11",
    "express": "^4.17.1",
    "lodash-es": "^4.17.15",
    "preact": "^10.2.1",
    "preact-router": "^3.2.1",
    "unistore": "^3.5.1",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.6.3",
